[
    {
      "id": "pull-can-scrapers",
      "execute-command": "git",
      "pass-arguments-to-command": [
        {
          "source": "string",
          "name": "pull"
        },
        {
          "source": "string",
          "name": "origin"
        },
        {
          "source": "string",
          "name": "main"
        }
      ],
      "command-working-directory": "/home/sglyon/can-scrapers",
      "trigger-rule": {
        "and": [
          {
            "match": {
              "type": "payload-hash-sha1",
              "secret": "PASSWORD",
              "parameter": {
                "source": "header",
                "name": "X-Hub-Signature"
              }
            }
          }
        ]
      }
    }
  ]
